    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "bs58": "^5.0.0",
    "chai": "^4.3.4",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "resolutions": {
